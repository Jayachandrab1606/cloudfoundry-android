<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.iosched.ui.widget.DashboardLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashboardActivity" >

    <TextView
        android:id="@+id/db_apps_btn"
        style="@style/DashboardButton"
        android:text="@string/db_apps_text"
        android:background="@drawable/abs__item_background_holo_light"
        android:drawableTop="@drawable/db_app" />

    <TextView
        android:id="@+id/db_services_btn"
        style="@style/DashboardButton"
        android:text="@string/db_services_text"
        android:background="@drawable/abs__item_background_holo_light"
        android:drawableTop="@drawable/db_services" />

    <TextView
        android:id="@+id/db_cloud_btn"
        style="@style/DashboardButton"
        android:text="@string/db_info_text"
        android:background="@drawable/abs__item_background_holo_light"
        android:drawableTop="@drawable/db_cloud" />

    <TextView
        android:id="@+id/db_about_btn"
        style="@style/DashboardButton"
        android:text="@string/db_about_text"
        android:background="@drawable/abs__item_background_holo_light"
        android:drawableTop="@drawable/db_info" />

</com.google.android.apps.iosched.ui.widget.DashboardLayout>